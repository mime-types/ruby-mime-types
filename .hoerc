---
exclude: !ruby/regexp '/
  \.(tmp|swp)$
  |
  \.(?:autotest|gemtest|gitignore|hoerc|minitest.rb|simplecov-prelude.rb)
  |
  ^(?i:TAGS)
  |
  \.(?:
      DS_Store|
      bundle|
      git|
      hg|
      idea|
      svn|
      vagrant
    )\/
  |
  [gG]emfile(?:\.lock)?
  |
  support\/
  |
  \.(?:coveralls|pullreview|travis|rubocop.*)\.yml
  |
  .byebug_history
  |
  \.gemspec$
  /x'
